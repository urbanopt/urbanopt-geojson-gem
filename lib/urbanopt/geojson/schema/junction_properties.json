{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://json-schema.org/openstudio-urban-modeling/junction_properties.json#",
    "title": "Junction object",
    "description": "Schema for a Junction object",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "description": "Unique id used to refer to this feature within this dataset, generated by database."
        },
        "fuel": {
            "type": "string",
            "enum": [
                "Electricity",
                "NaturalGas",
                "Water"
            ]
        },
        "geojson_id": {
            "description": "Id of the geojson for this junction, managed by the database.",
            "type": "string"
        }
    },
    "required": [
        "fuel"
    ]
}